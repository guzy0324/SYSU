/* EXAMPLE 10: all testing */

program p6;
class Test {
        method void main()
	     declarations
			int x;
                	int i;
         enddeclarations 
	{
		system.readln(x);
		if (x > 10)
			{
			i := 1;
			};
		if (x > 6)
			{
	 		i := 2;
			}
		else if (x > 3)
			{
			i := 3;
			}
		else
			{
			i := 4;	
			};
       system.println('i=');
	   system.println(i);
	}
}

