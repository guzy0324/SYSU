/* ex3: if-else, while */
program ex3;
class c3
{
	method void main()
	declarations
		int x;
		int y;
	enddeclarations
	{
	system.println('x=?');
	system.readln(x);
	if (x>10)
		{
		system.println('>10');
		}
	else if (x>5)
		{
		system.println('>5');
		}
	else
		{
		system.println('<=5');
		};

	x := 1;
	y := 0;
	while (x <= 10)
		{
		y := y + x;
		x := x + 1;
		};
	system.println('y=');
	system.println(y);
	}
}

